class Solution:
    def uniquePaths(self, m: int, n: int) -> int:
        paths = [0] * m * n
        paths[0] = 1
        for i in range(m):
            for j in range(n):
                if i + 1 < m:
                    paths[i + 1 + j * m] += paths[i + j * m]
                if j + 1 < n:
                    paths[i + (j + 1) * m] += paths[i + j * m]
            
            
        return paths[-1]
